                                                    <SkewTransform/>
                                                    <RotateTransform/>
                                                    <TranslateTransform/>
                                                </TransformGroup>
                                            </Polygon.RenderTransform>
                                            <Polygon.Points>
                                                <MultiBinding Converter="{StaticResource HapticEventToPointsConverter}" PresentationTraceSources.TraceLevel="High">
                                                    <Binding ElementName="TopAxis" Path="ActualHeight"/>
                                                    <Binding />

                                <ItemsControl.ItemsPanel>
                                    <ItemsPanelTemplate>
                                    <ZoomableCanvas x:Name="PeriodicSqueezeAxis" Background="#FF3F3F46" />
                                    </ItemsPanelTemplate>
                                </ItemsControl.ItemsPanel>
                                <ItemsControl.ItemTemplate>
                                    <DataTemplate>
                                    <ContentControl >

                                <ItemsControl.ItemsPanel>
                                    <ItemsPanelTemplate>
                                    <ZoomableCanvas x:Name="StopAxis" Background="#FF2D2D30" />
                                    </ItemsPanelTemplate>
                                </ItemsControl.ItemsPanel>
                                <ItemsControl.ItemTemplate>
                                    <DataTemplate>
                                    <ContentControl >
                                        <Polygon x:Name="polygon" Stroke="#FF686868" RenderTransformOrigin="0.0,0.0">
                                            <Polygon.RenderTransform>
                                                <TransformGroup>
